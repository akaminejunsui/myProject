(function cyseal(){		
	var part1 = "://cyseal"; 
	var part2 = ".cyveillance";
	var part3 = ".com";	
	var port = "";
	var part4 = "/SiteSeal";
	var part5 = "/cyss.js";
		
	try{
		var protocol = ('https:'   == document.location.protocol ? 'https'   : 'http');
	
		var x = protocol + part1 + part2 + part3 + port + part4 + part5;
			
		var gt_is=x+'?ref='+window.location.hostname;		
				
		(function() {
		    var cys = document.createElement('script'); 
		    cys.type = 'text/javascript';
		    cys.async = true;
		    cys.src = gt_is;
		    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(cys, s);
		})();
	}
	catch(e){}		
})();
